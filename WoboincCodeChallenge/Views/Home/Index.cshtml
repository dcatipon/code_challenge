@{
    ViewBag.Title = "Home";
}
<div class="well text-center">
    <form id="searchForm" class="form-inline" role="form">
        <div class="form-group">
            <label class="sr-only" for="searchBox">Search input</label>
            <input type="search" class="form-control searchBox" id="searchBox" placeholder="Enter book information">
        </div>
        <button id="searchSubmit" type="submit" class="btn btn-default">Search Books</button>
        <img src="http://books.google.com/googlebooks/images/poweredby.png" /></form>
</div>
<div hidden id="searchResultsPanel" class="panel panel-default scroll">
    <div class="col-lg-12">
        <h3>Search Results</h3>
    </div>
    <hr />
    <div id="content"></div>
    <div id="navPager" class="col-lg-12">
        <ul class="pager">
            <li><a id="previousPage" href="#">Previous</a></li>
            <li><a id="nextPage" href="#">Next</a></li>
        </ul>
    </div>
</div>